{
  "name": "react-flux-ts",
  "version": "1.0.0",
  "description": "react react-router flux typescript webpack",
  "main": "./src/index.tsx",
  "scripts": {
    "mocked-backend": "json-server -s dist -p 8080 -w config/json-server.json",
    "test": "tslint -c config/tslint.json 'src/**/*.tsx'",
    "start": "npm i; npm run dev-in-parallel",
    "dev-in-parallel": "npm-run-all --parallel wp:dev mocked-backend",
    "bs": "npm i; npm-run-all wp:compile start-in-parallel",
    "start-in-parallel": "npm-run-all --parallel wp:watch bs-server mocked-backend",
    "bs-server": "browser-sync start --config=./config/browser-sync.js",
    "wp:dev": "webpack-dev-server --config ./config/webpack.js -w --hot",
    "wp:build": "webpack --config ./config/webpack.js -p",
    "wp:watch": "webpack --config ./config/webpack.js -w",
    "wp:compile": "webpack --config ./config/webpack.js",
    "tsc:watch": "tsc -w -p ./src",
    "tsc:compile": "tsc -p ./src",
    "postinstall": "npm-run-all typings:*",
    "typings:rm": "rimraf typings*",
    "typings:init": "typings init ./src",
    "typings:vendor": "typings install --global --save dt~jquery dt~bootstrap",
    "typings:react": "typings i -G --save dt~react dt~react-dom dt~react-router dt~react-router/history dt~flux",
    "commit": "commit-wizard"
  },
  "config": {
    "pre-git": {
      "pre-push": [
        "npm test"
      ]
    }
  },
  "author": "Maksim Kostromin",
  "license": "ISC",
  "peerDependencies": {
    "react": "^15.1.0 || ^0.14.0",
    "react-router": "^2.4.1 || ^2.0.0"
  },
  "devDependencies": {
    "browser-sync": "^2.13.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "cssnano": "^3.7.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.19.0",
    "http-proxy-middleware": "^0.15.2",
    "json-server": "^0.8.14",
    "npm-run-all": "^2.1.1",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "pre-git": "^3.9.0",
    "rimraf": "^2.5.2",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "tslint": "^3.11.0",
    "typescript": "^1.8.10",
    "typings": "^1.0.4",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "bootstrap": "^3.3.6",
    "flux": "^2.1.1",
    "jquery": "^3.0.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-router": "^2.4.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daggerok/react-flux-ts.git"
  },
  "keywords": [
    "react",
    "react-router",
    "typescript",
    "webpack",
    "flux"
  ],
  "bugs": {
    "url": "https://github.com/daggerok/react-flux-ts/issues"
  },
  "homepage": "https://github.com/daggerok/react-flux-ts#readme"
}
